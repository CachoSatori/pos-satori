{
  "name": "pos-satori",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build && npm run sentry:sourcemaps",
    "preview": "vite preview",
    "test": "vitest --environment=jsdom",
    "test:unit": "vitest",
    "test:unit:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org satori-3g --project javascript-react dist && sentry-cli sourcemaps upload --org satori-3g --project javascript-react dist",
    "cypress:open": "NODE_NO_WARNINGS=1 cypress open",
    "cypress:run": "NODE_NO_WARNINGS=1 cypress run",
    "test:e2e": "NODE_NO_WARNINGS=1 cypress run",
    "lint": "eslint . --ext .ts,.tsx"
  },
  "dependencies": {
    "@firebase/app": "^0.13.2",
    "@firebase/auth": "^1.10.8",
    "@firebase/firestore": "^4.8.0",
    "@firebase/messaging": "^0.12.22",
    "@firebasegen/default-connector": "file:dataconnect-generated/js/default-connector",
    "@sentry/browser": "^9.36.0",
    "@sentry/cli": "^2.47.0",
    "@sentry/react": "^9.37.0",
    "@sentry/tracing": "^7.120.3",
    "axios": "^1.10.0",
    "chart.js": "^4.5.0",
    "date-fns": "^4.1.0",
    "exceljs": "^4.4.0",
    "firebase": "^11.10.0",
    "firebase-admin": "^12.7.0",
    "i18next": "^25.3.1",
    "i18next-browser-languagedetector": "^8.2.0",
    "react": "^19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-datepicker": "^8.4.0",
    "react-dom": "^19.1.0",
    "react-i18next": "^15.6.0",
    "react-router-dom": "^7.6.3",
    "react-toastify": "^11.0.5",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/dotenv": "^6.1.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-router-dom": "^5.3.3",
    "@types/testing-library__react": "^10.0.1",
    "@vitejs/plugin-react": "^4.5.2",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "cypress": "^14.5.1",
    "cypress-firebase": "^4.2.1",
    "dotenv": "^17.1.0",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.11",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^7.0.0",
    "vitest": "^3.2.4"
  }
}
