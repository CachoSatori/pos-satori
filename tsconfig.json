{
       "compilerOptions": {
         "jsx": "react-jsx",
         "module": "esnext",
         "target": "esnext",
         "strict": true,
         "moduleResolution": "node",
         "esModuleInterop": true,
         "skipLibCheck": true,
         "forceConsistentCasingInFileNames": true,
         "allowSyntheticDefaultImports": true,
         "resolveJsonModule": true,
         "isolatedModules": true,
         "noEmit": true,
         "types": ["vite/client"]
       },
       "include": ["src"],
       "exclude": ["node_modules", "dist"]
     }